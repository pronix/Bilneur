<% stylesheet "slider/general", "slider/SpryCollapsiblePanel", "slider/SpryTabbedPanels4.css" %>
<% javascript "jquery.custom_radio_checkbox", "slider/SpryCollapsiblePanel", "slider/SpryTabbedPanels", "product_index" %>

<% if @products || @deals %>
  <%= render :partial => 'shared/products', :locals => {:products => (@products || @deals), :short_description => true} %>
<% elsif @sellers %>
  <%= render :partial => 'sellers', :locals => {:sellers => @sellers} %>
<% end %>
