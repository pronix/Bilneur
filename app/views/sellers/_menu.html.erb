<% stylesheet "slider/general", "slider/SpryCollapsiblePanel", "slider/SpryTabbedPanels2","slider/SpryTabbedPanels3", "product" %>
<% javascript "jquery.custom_radio_checkbox", "slider/SpryCollapsiblePanel", "slider/SpryTabbedPanels", "product_index" %>

<ul class="TabbedPanelsTabGroup">
  <li class="TabbedPanelsTab" >
    <span class="TabbedPanelsTabSelected">
      <%= link_to_unless_current "Store", store_seller_path(@seller), {:class => "TabbedPanelsTab" } %>
    </span>
  </li>

  <li class="TabbedPanelsTab" tabindex="0">
    <span class="TabbedPanelsTabSelected">
      <%= link_to_unless_current "About Seller", about_seller_path(@seller), {:class => "TabbedPanelsTab"}  %>
    </span>
  </li>
  <li class="TabbedPanelsTab" tabindex="0">
    <span class="TabbedPanelsTabSelected">
      <%= link_to_unless_current "Feedback", feedback_seller_path(@seller), {:class => "TabbedPanelsTab"}  %>
    </span>
  </li>
  <li class="TabbedPanelsTab" tabindex="0">
    <span class="TabbedPanelsTabSelected">
      <%= link_to_unless_current "Return Policy", return_policy_seller_path(@seller), {:class => "TabbedPanelsTab"}  %>
    </span>
  </li>
  <li class="TabbedPanelsTab" tabindex="0">
    <span>Specials</span>
  </li>
  <li class="TabbedPanelsTab" tabindex="0">
    <span>Shipping</span>
  </li>
  <li class="TabbedPanelsTab" tabindex="0">
    <span class="TabbedPanelsTabSelected">
      <%= link_to_unless_current "Contact Seller",customer_service_seller_path(@seller), {:class => "TabbedPanelsTab"}  %>
    </span>
  </li>
</ul>
