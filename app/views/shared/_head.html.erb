<title><%= title %></title>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
<%== meta_data_tags %>
<%= stylesheet_link_tag "skin", "dd2", :media => "screen, projection" %>
<!-- [if lt IE 8]><%= stylesheet_link_tag "ie.css", :media => "screen, projection" %><![endif]-->
<%#== stylesheet_tags %>
<%= csrf_meta_tag %>
<%= javascript_include_tag 'jquery-1.4.2.min', 'jquery.validate.min.js', 'jquery.dd', 'rails', 'application', :cache => 'jquery-core' %>
<% unless I18n.locale.to_s[0..1] == 'en' %>
  <%= javascript_include_tag ['localization/messages_', I18n.locale.to_s[0..1], '.js'].join %>
<% end %>
<%= yield :head %>
